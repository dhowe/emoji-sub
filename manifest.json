{
  "manifest_version": 2,
  "name": "emoji-sub",
  "description": "Replaces words with emojis",
  "homepage_url": "https://github.com/dhowe/emoji-sub",
  "version": "0.1",
  "icons": {
    "48": "icons/icon.png",
    "96": "icons/icon@2x.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "./sub-emoji.js"
      ]
    }
  ]
}
