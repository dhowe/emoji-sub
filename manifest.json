{
  "manifest_version": 2,
  "name": "emoji-sub",
  "description": "Replaces words with emojis",
  "homepage_url": "https://github.com/dhowe/emoji-sub",
  "version": "0.1",
  "browser_action": {
    "default_icon": {
      "16": "icon.png",
      "32": "icon.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "sub-emoji.js"
      ]
    }
  ]
}